   "source": [
    "## 4. Prepare the Data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "d50ed6e3-da2c-47b5-827c-378ec2885c13",
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "housing = pd.read_csv(\"housing_clean.csv\")  # 불러오기"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "72b0a9e3-8454-48e6-9e5f-4db52ccc056c",
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn.model_selection import StratifiedShuffleSplit\n",
    "\n",
    "# 원본 데이터 로딩\n",
    "housing = pd.read_csv(\"datasets/housing/housing.csv\")\n",
    "\n",
    "# 소득 구간 컬럼 생성\n",
    "housing[\"income_cat\"] = np.ceil(housing[\"median_income\"] / 1.5)\n",
    "housing[\"income_cat\"] = housing[\"income_cat\"].where(housing[\"income_cat\"] < 5, 5.0)\n",
    "\n",
    "# 계층적 샘플링으로 훈련/테스트 분할\n",
    "split = StratifiedShuffleSplit(n_splits=1, test_size=0.2, random_state=42)\n", # 무작위 분할
    "for train_idx, test_idx in split.split(housing, housing[\"income_cat\"]):\n",
    "    strat_train_set = housing.loc[train_idx]\n",
    "    strat_test_set = housing.loc[test_idx]\n",
    "\n",
    "# 임시로 생성한 소득 카테고리 삭제\n",
    "for set_ in (strat_train_set, strat_test_set):\n",
    "    set_.drop(\"income_cat\", axis=1, inplace=True)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "73182fe0-85d1-4ce8-9d71-f4811f6b6b09",
   "metadata": {},
   "outputs": [],
   "source": [
    "housing = strat_train_set.drop(\"median_house_value\", axis=1)  # 입력 특성\n",
    "housing_labels = strat_train_set[\"median_house_value\"].copy()  # 타겟 값"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "8a8e56c2-1675-494a-bd4f-5e9e80504c2c",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.impute import SimpleImputer  # 결측값 처리를 위한 클래스 임포트\n",
    "\n",
    "housing_num = housing.drop(\"ocean_proximity\", axis=1)  # 수치형 특성만 선택 (문자열 컬럼 제거)\n",
    "imputer = SimpleImputer(strategy=\"median\")  # 결측값을 각 열의 중간값으로 대체하도록 설정\n",
    "imputer.fit(housing_num)  # 훈련 데이터에 대해 중간값 계산 (각 열마다)\n",
    "X = imputer.transform(housing_num)  # 계산된 중간값으로 결측값 대체 (NumPy 배열 반환)\n",
    "housing_tr = pd.DataFrame(X, columns=housing_num.columns)  # NumPy 배열을 원래 열 이름과 함께 DataFrame으로 변환"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "4ccf7ee9-5c6a-4ef7-b0f1-e75cf926ed30",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.preprocessing import OneHotEncoder  # 범주형 변수를 One-Hot 인코딩하기 위한 클래스 임포트\n",
    "\n",
    "housing_cat = housing[[\"ocean_proximity\"]]  # 범주형 특성만 DataFrame 형태로 추출\n",
    "cat_encoder = OneHotEncoder(sparse_output=False)  # 희소행렬이 아닌 일반 배열로 결과 반환하도록 설정\n",
    "housing_cat_1hot = cat_encoder.fit_transform(housing_cat)  # 카테고리 값을 One-Hot 인코딩으로 변환"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "eac2d718-f157-423c-8ac9-9438a671152b",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.base import BaseEstimator, TransformerMixin  # 사용자 정의 변환기를 만들기 위한 기본 클래스들\n",
    "\n",
    "# 열 인덱스 설정\n",
    "rooms_ix, bedrooms_ix, population_ix, household_ix = 3, 4, 5, 6  # 순서대로 total_rooms, total_bedrooms, population, households 열의 인덱스\n",
    "\n",
    "class CombinedAttributesAdder(BaseEstimator, TransformerMixin):  # 파생 특성 생성용 사용자 정의 변환기 클래스 정의\n",
    "    def __init__(self, add_bedrooms_per_room=True):  # bedrooms_per_room 특성을 추가할지 여부를 설정하는 하이퍼파라미터\n",
    "        self.add_bedrooms_per_room = add_bedrooms_per_room\n",
    "    def fit(self, X, y=None):  # fit 메서드는 아무 작업 없이 self 반환 (필수 메서드)\n",
    "        return self\n",
    "    def transform(self, X, y=None):  # transform 메서드에서 새로운 파생 특성들을 계산하여 기존 데이터에 추가\n",
    "        rooms_per_household = X[:, rooms_ix] / X[:, household_ix]\n",
    "        population_per_household = X[:, population_ix] / X[:, household_ix]\n",
    "        if self.add_bedrooms_per_room:\n",
    "            bedrooms_per_room = X[:, bedrooms_ix] / X[:, rooms_ix]\n",
    "            return np.c_[X, rooms_per_household, population_per_household, bedrooms_per_room]  # 세 개의 파생 특성 추가\n",
    "        else:\n",
    "            return np.c_[X, rooms_per_household, population_per_household]  # 두 개의 파생 특성만 추가"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "46d3c242-8200-42ec-9c7d-ca69b19e77bb",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.preprocessing import StandardScaler  # 특성들을 표준화(평균 0, 표준편차 1)하기 위한 클래스 임포트\n",
    "scaler = StandardScaler()  # 표준화 객체 생성\n",
    "housing_scaled = scaler.fit_transform(housing_tr)  # 학습과 변환을 한 번에 수행하여 스케일링된 데이터 생성"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "67bfff41-5c39-4ff8-bb2c-960ca1c89e19",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.pipeline import Pipeline  # 여러 전처리 과정을 순차적으로 실행할 수 있는 파이프라인 클래스 임포트\n",
    "\n",
    "num_pipeline = Pipeline([\n",
    "    ('imputer', SimpleImputer(strategy=\"median\")),  # 결측값을 각 열의 중간값으로 대체\n",
    "    ('attribs_adder', CombinedAttributesAdder()),  # 파생 특성 추가\n",
    "    ('std_scaler', StandardScaler()),  # 표준화 (평균 0, 표준편차 1)\n",
    "])\n",
    "\n",
    "housing_num_tr = num_pipeline.fit_transform(housing_num)  # 수치형 데이터에 파이프라인 적용 (학습 + 변환)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "00fbea31-ad8c-492a-9d50-87f811f09cf3",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.pipeline import Pipeline  # 전처리 단계를 순차적으로 처리하는 파이프라인 클래스 임포트\n",
    "\n",
    "num_pipeline = Pipeline([\n",
    "    ('imputer', SimpleImputer(strategy=\"median\")),  # 결측값을 중간값으로 대체\n",
    "    ('attribs_adder', CombinedAttributesAdder()),  # 파생 특성 추가\n",
    "    ('std_scaler', StandardScaler()),  # 특성 표준화 (평균 0, 표준편차 1)\n",
    "])\n",
    "\n",
    "housing_num_tr = num_pipeline.fit_transform(housing_num)  # 수치형 데이터에 파이프라인 적용 (학습 및 변환 수행)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "4b380f51-d3d1-473b-8792-cd5d92bd1b76",
   "metadata": {},
   "outputs": [],
   "source": [
    "class DataFrameSelector(BaseEstimator, TransformerMixin):  # 지정한 열만 선택하는 사용자 정의 변환기 클래스 정의\n",
    "    def __init__(self, attribute_names):  # 선택할 열 이름 목록을 초기화\n",
    "        self.attribute_names = attribute_names\n",
    "    def fit(self, X, y=None):  # fit 메서드 (여기선 학습 필요 없음)\n",
    "        return self\n",
    "    def transform(self, X):  # 지정된 열만 NumPy 배열 형태로 추출하여 반환\n",
    "        return X[self.attribute_names].values"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "4d8a20e2-2a01-4f2f-a53d-d498f4fe9cbe",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.pipeline import FeatureUnion  # 여러 변환기 파이프라인을 병렬로 결합하는 클래스 임포트\n",
    "\n",
    "num_attribs = list(housing_num)  # 수치형 특성 이름 목록\n",
    "cat_attribs = [\"ocean_proximity\"]  # 범주형 특성 이름 목록\n",
    "\n",
    "num_pipeline = Pipeline([\n",
    "    ('selector', DataFrameSelector(num_attribs)),  # 수치형 열 선택\n",
    "    ('imputer', SimpleImputer(strategy=\"median\")),  # 결측값을 중간값으로 대체\n",
    "    ('attribs_adder', CombinedAttributesAdder()),  # 파생 특성 추가\n",
    "    ('std_scaler', StandardScaler()),  # 표준화 (평균 0, 표준편차 1)\n",
    "])\n",
    "\n",
    "cat_pipeline = Pipeline([\n",
    "    ('selector', DataFrameSelector(cat_attribs)),  # 범주형 열 선택\n",
    "    ('onehot', OneHotEncoder(sparse_output=False)),  # One-Hot 인코딩 적용 (밀집 배열 반환)\n",
    "])\n",
    "\n",
    "full_pipeline = FeatureUnion(transformer_list=[\n",
    "    (\"num_pipeline\", num_pipeline),  # 수치형 파이프라인\n",
    "    (\"cat_pipeline\", cat_pipeline),  # 범주형 파이프라인\n",
    "])  # 두 파이프라인의 출력을 수평 결합하여 하나의 전처리 결과로 반환"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "23a498b6-0337-43c0-9cb3-470d3ee287e3",
   "metadata": {},
   "outputs": [],
   "source": [
    "housing_prepared = full_pipeline.fit_transform(housing)  # 전체 파이프라인을 학습시키고 housing 데이터에 적용하여 전처리된 최종 특성 행렬 생성"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
   "id": "037be194-81d9-45ed-ba18-f9cdb15a9f30",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "['data/housing_raw.pkl']"
      ]
     },
     "execution_count": 42,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "import joblib\n",
    "joblib.dump(housing_prepared, \"data/housing_prepared.pkl\")\n",
    "joblib.dump(housing_labels, \"data/housing_labels.pkl\")\n",
    "full_pipeline.fit(housing)  # housing은 DataFrame 형태\n",
    "joblib.dump(full_pipeline, \"models/full_pipeline.pkl\")\n",
    "joblib.dump(housing, \"data/housing_raw.pkl\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "id": "d003a6d6-7e74-43d7-9548-460c3ca87152",
